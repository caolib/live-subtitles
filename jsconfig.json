{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"src/*"
			]
		},
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"jsx": "preserve",
		"jsxImportSource": "vue",
		"strict": true,
		"noImplicitAny": false,
		"skipLibCheck": true,
		"types": [
			"vite/client"
		]
	},
	"include": [
		"src/**/*",
		"src/**/*.vue",
		"types/**/*"
	],
	"exclude": [
		"node_modules",
		"dist",
		"src-tauri"
	]
}